          override public function update():void
    {
  //speler snelheid en de game controls
    player.acceleration.x = 0;
   if (FlxG.keys.LEFT)
    {
        player.acceleration.x =- player.maxVelocity.x * 3;
    }
     if (FlxG.keys.RIGHT)
    {
        player.acceleration.x = player.maxVelocity.x * 3;
    }
     
	if (FlxG.keys.justPressed("UP") && player.isTouching(FlxObject.FLOOR))
    {
        player.velocity.y = -120;
    }
	
	if (FlxG.keys.justPressed("ENTER"))
    {
        System.exit(0);
    }
	
 //check of de speler muntje aanraakt
    FlxG.overlap(coins, player, getCoin);
 
 //check of de speler de uitgang aanraakt
    FlxG.overlap(exit, player, win);
 
   //check of de speler een bom aanraakt
    FlxG.overlap(bombs, player, touchBomb);
   
	//roep de animatie functie aan
	doAnimation();
       
	//update alle objecten
        super.update();
		
		//check of de speler met het level botst
		FlxG.collide(level, player);
    }
