	public function doAnimation():void
 {
	 //links lopen
	 if (FlxG.keys.LEFT)
	 {
		 player.facing = FlxObject.LEFT;
		 }
		 //rechts lopen
		 else if (FlxG.keys.RIGHT)
		 {
			 player.facing = FlxObject.RIGHT;
			 }
			 if (player.isTouching(FlxObject.FLOOR))
			 {
				 if (FlxG.keys.LEFT || FlxG.keys.RIGHT)
				 {
					 player.play("run");
					 }
					 // idle
					 else
					 {
						 player.play("idle");
						 }
						 }
						 //jump
						 else
						 {
							 player.play("jump");
							 }
					     }
    }
